<p id="notice"><%= notice %></p>

<h1>Listing Basico Planes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @basico_planes.each do |basico_plan| %>
      <tr>
        <td><%= basico_plan.nombre %></td>
        <td><%= link_to 'Show', basico_plan %></td>
        <td><%= link_to 'Edit', edit_basico_plan_path(basico_plan) %></td>
        <td><%= link_to 'Destroy', basico_plan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Plan', new_basico_plan_path %>
