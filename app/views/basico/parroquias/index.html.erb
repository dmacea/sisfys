 
  
 

<div class="row">
    <div class="col-md-4"  >
      <%= link_to 'Nuevo', new_basico_parroquia_path,
        class: "btn btn-default btn-sm" %>
    </div>
    <div class="col-md-8" align="right">
        <%= buscar(basico_parroquias_path,["Nombre","Pais","Estado","Municipio"]) %>
    </div>
</div>

<h3>Parroquias <small> listado</small></h3>
<p id="notice"><%= notice %></p>

<div class="table-responsive">
    <table class="table table-striped">
        <%=table_head([{title:"Nombre"},{title:"Pais"},{title:"Estado"},{title:"Municipio"}]) %>
       <tbody>
        <%= render  @parroquias %>
        </tbody>
    </table>
</div> 

<%= paginador(@parroquias) %>
