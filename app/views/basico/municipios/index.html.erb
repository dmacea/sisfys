
<div class="row">
    <div class="col-md-4"  >
      <%= link_to 'Nuevo', new_basico_municipio_path,
        class: "btn btn-default btn-sm" %>
    </div>
    <div class="col-md-8" align="right">
        <%= buscar(basico_municipios_path,["Nombre","Estado"]) %>
    </div>
</div>

<h3>Municipios <small> listado</small></h3>
<p id="notice"><%= notice %></p>

<div class="table-responsive">
    <table class="table table-striped">
        <%=table_head([{title:"Nombre"},{title:"Pais"},{title:"Estado"}]) %>
       <tbody>
        <%= render  @municipios %>
        </tbody>
    </table>
</div> 

<%= paginador(@municipios) %>
   