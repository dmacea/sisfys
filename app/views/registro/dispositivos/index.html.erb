<p id="notice"><%= notice %></p>

<h1>Listing Registro Dispositivos</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th>Modelo</th>
      <th>Serial</th>
      <th>Email</th>
      <th>Linea</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registro_dispositivos.each do |registro_dispositivo| %>
      <tr>
        <td><%= registro_dispositivo.tipo %></td>
        <td><%= registro_dispositivo.modelo %></td>
        <td><%= registro_dispositivo.serial %></td>
        <td><%= registro_dispositivo.email %></td>
        <td><%= registro_dispositivo.linea %></td>
        <td><%= link_to 'Show', registro_dispositivo %></td>
        <td><%= link_to 'Edit', edit_registro_dispositivo_path(registro_dispositivo) %></td>
        <td><%= link_to 'Destroy', registro_dispositivo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dispositivo', new_registro_dispositivo_path %>
