<p id="notice"><%= notice %></p>

<h1>Listing Registro Cliente Servicios</h1>

<table>
  <thead>
    <tr>
      <th>Cliente</th>
      <th>Servicio</th>
      <th>Dispositivo</th>
      <th>Pagaplan</th>
      <th>Fechainicio</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registro_cliente_servicios.each do |registro_cliente_servicio| %>
      <tr>
        <td><%= registro_cliente_servicio.cliente %></td>
        <td><%= registro_cliente_servicio.servicio %></td>
        <td><%= registro_cliente_servicio.dispositivo %></td>
        <td><%= registro_cliente_servicio.pagaPlan %></td>
        <td><%= registro_cliente_servicio.fechaInicio %></td>
        <td><%= registro_cliente_servicio.estado %></td>
        <td><%= link_to 'Show', registro_cliente_servicio %></td>
        <td><%= link_to 'Edit', edit_registro_cliente_servicio_path(registro_cliente_servicio) %></td>
        <td><%= link_to 'Destroy', registro_cliente_servicio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

